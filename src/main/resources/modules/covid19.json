{
  "name": "covid19",
  "remarks": [
    "This is module models the infection of COVID-19 patients during the 2020 global pandemic."
  ],
  "states": {
    "Initial": {
      "type": "Initial",
      "direct_transition": "Wait Until Outbreak"
    },
    "Terminal": {
      "type": "Terminal"
    },
    "Wait Until Outbreak": {
      "allow": {
        "condition_type": "Date",
        "operator": "==",
        "year": 2020
      },
      "type": "Guard",
      "direct_transition": "Wait Until Exposure"
    },
    "Wait Until Exposure": {
      "range": {
        "low": 1,
        "high": 90,
        "unit": "days"
      },
      "type": "Delay",
      "distributed_transition": [
        {
          "distribution": 0.2,
          "transition": "Call Infection Submodule"
        },
        {
          "distribution": 0.8,
          "transition": "Terminal"
        }
      ]
    },
    "Call Infection Submodule": {
      "submodule": "covid19/infection",
      "type": "CallSubmodule",
      "direct_transition": "Terminal"
    }
  }
}