{
    "name": "Blood Pressure Cuff",
    "remarks": [
      "This module simulates a blood pressure cuff."
    ],
    "states": {
      "Initial": {
        "type": "Initial",
        "direct_transition": "BloodPressureEncounter"
      },
      "BloodPressureEncounter": {
        "type": "Encounter",
        "wellness": true,
        "direct_transition": "StartBloodPressure"
      },
      "StartBloodPressure": {
        "type": "Observation",
        "codes": [
          {
            "system": "http://loinc.org",
            "code": "8480-6",
            "display": "Systolic blood pressure"
          },
          {
            "system": "LOINC",
            "code": "8462-4",
            "display": "Diastolic blood pressure"
          }
        ],
        "direct_transition": "End_BloodPressureEncounter"
      },
      "End_BloodPressureEncounter": {
        "type": "Terminal",
        "name": "Terminal"
      }
    },
    "gmf_version": 1
  }